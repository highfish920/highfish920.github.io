    <link href="http://yoursite.com/2020/08/12/%E8%A7%A3%E5%86%B3oracle%E6%95%B0%E6%8D%AE%E5%BA%93%E4%B8%AD%E6%96%87%E4%B9%B1%E7%A0%81%E9%97%AE%E9%A2%98/"/>
    <id>http://yoursite.com/2020/08/12/%E8%A7%A3%E5%86%B3oracle%E6%95%B0%E6%8D%AE%E5%BA%93%E4%B8%AD%E6%96%87%E4%B9%B1%E7%A0%81%E9%97%AE%E9%A2%98/</id>
